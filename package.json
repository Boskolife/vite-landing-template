{
  "name": "vite-landing-template",
  "private": true,
  "version": "0.9.0",
  "type": "module",
  "description": "Modern landing page builder with Vite, JavaScript, SCSS, and Handlebars",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "type-check": "echo \"Type checking not needed for JavaScript\"",
    "changelog:check": "node .cursor/rules/changelog-check.js",
    "changelog:update": "node .cursor/rules/changelog-auto-update.js",
    "pre-commit": "npm run lint && npm run build && npm run changelog:check",
    "prepare": "husky",
    "release": "node scripts/release.js",
    "release:patch": "npm run release patch",
    "release:minor": "npm run release minor",
    "release:major": "npm run release major"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.7",
    "imagemin": "^9.0.1",
    "imagemin-webp": "^8.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sharp": "^0.34.4",
    "terser": "^5.44.0",
    "vite": "^7.1.7",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "dependencies": {
    "swiper": "^12.0.2"
  },
  "keywords": [
    "vite",
    "javascript",
    "scss",
    "handlebars",
    "landing-page",
    "template"
  ],
  "author": "",
  "license": "MIT"
}
